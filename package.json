{
  "name": "@tobshub/browser-file-system",
  "version": "1.0.0",
  "description": "File system for your browser",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "author": "Tobshub",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c"
  },
  "devDependencies": {
    "esbuild": "^0.17.11",
    "rollup": "^3.19.1",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "url": "https://github.com/Tobshub/browser-file-system"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "keywords": [
    "browser",
    "file",
    "system"
  ],
  "files": [
    "./dist/*"
  ],
  "dependencies": {
    "localforage": "^1.10.0"
  }
}
